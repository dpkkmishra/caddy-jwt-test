{
  "admin": {
    "disabled": false,
    "config": {
      "persist": false
    }
  },
  "logging": {
    "logs": {
      "default": {
        "level": "DEBUG"
      }
    }
  },
  "apps": {
    "http": {
      "http_port": 3080,
      "https_port": 3443,
      "servers": {
        "srv0": {
          "listen": [
            ":3443"
          ],
          "routes": [
            {
              "handle": [
                {
                  "handler": "authentication",
                  "providers": {
                    "jwt": {
                      "authorizer": {
                        "primary": true,
                        "trusted_tokens": [
                          {
                            "token_name": "access_token",
                            "token_secret": "383aca9a-1c39-4d7a-b4d8-67ba4718dd3f"
                          },
                          {
                            "token_rsa_file": "testdata/rskeys/test_1.pem"
                          }
                        ],
                        "auth_url_path": "/auth",
                        "access_list": [
                          {
                            "action": "allow",
                            "claim": "roles",
                            "values": [
                              "anonymous",
                              "guest",
                              "admin"
                            ]
                          }
                        ],
                        "strip_token": false,
                        "pass_claims": false,
                        "token_types": [
                          "HS256",
                          "HS384",
                          "HS512"
                        ],
                        "token_sources": [
                          "header",
                          "cookie",
                          "query"
                        ]
                      }
                    }
                  }
                },
                {
                  "body": "prometheus",
                  "handler": "static_response",
                  "status_code": 200
                }
              ],
              "match": [
                {
                  "path": [
                    "/prometheus"
                  ]
                }
              ],
              "terminal": true
            },
            {
              "handle": [
                {
                  "handler": "authentication",
                  "providers": {
                    "jwt": {
                      "authorizer": {
                        "access_list": [
                          {
                            "action": "allow",
                            "claim": "roles",
                            "values": [
                              "anonymous",
                              "guest",
                              "admin"
                            ]
                          }
                        ]
                      }
                    }
                  }
                },
                {
                  "body": "alertmanager",
                  "handler": "static_response",
                  "status_code": 200
                }
              ],
              "match": [
                {
                  "path": [
                    "/alertmanager"
                  ]
                }
              ],
              "terminal": true
            },
            {
              "handle": [
                {
                  "body": "1.0.0",
                  "handler": "static_response",
                  "status_code": 200
                }
              ],
              "match": [
                {
                  "path": [
                    "/version"
                  ]
                }
              ],
              "terminal": true
            }
          ],
          "tls_connection_policies": [
            {
              "certificate_selection": {
                "any_tag": [
                  "cert0"
                ]
              }
            }
          ]
        }
      }
    },
    "tls": {
      "certificates": {
        "load_files": [
          {
            "certificate": "/caddy.localhost.crt",
            "key": "/caddy.localhost.key",
            "tags": [
              "cert0"
            ]
          }
        ]
      }
    },
    "pki": {
      "certificate_authorities": {
        "local": {
          "install_trust": false
        }
      }
    }
  }
}
